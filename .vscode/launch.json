{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Application 1",
      "type": "node",
      "program": "${workspaceFolder}/app1/main.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "Launch Application 2",
      "type": "java",
      "request": "launch",
      "mainClass": "com.example.demo.DemoApplication",
      "projectName": "demo"
    }
  ],
  "env": {
    "TITLE1": "Hello, Node!",
    "TITLE2": "Hello, Java!",
    "A1URL": "trial-dev-mta-intro-app1.cfapps.us10-001.hana.ondemand.com",
    "A2URL": "trial-dev-mta-intro-app2.cfapps.us10-001.hana.ondemand.com"
  }
}
