{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Application 1",
      "type": "node",
      "program": "${workspaceFolder}/app1/main.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "env": {
        "TITLE": "Hello Node!",
        // replace with the url exposed by Business Application Studio
        "A2URL": "https://port8080-workspaces-ws-knhqs.us10.trial.applicationstudio.cloud.sap/"
      }
    },
    {
      "name": "Launch Application 2",
      "type": "java",
      "request": "launch",
      "mainClass": "com.example.demo.DemoApplication",
      "projectName": "demo",
      "env": {
        "TITLE": "Hello Java!",
        // replace with the url exposed by Business Application Studio
        "A1URL": "https://port3002-workspaces-ws-knhqs.us10.trial.applicationstudio.cloud.sap/"
      }
    }
  ]
}
