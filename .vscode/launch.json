{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Application 1",
      "type": "node",
      "program": "${workspaceFolder}/app1/main.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "env": {
        "TITLE": "Hello Node!",
        "A2URL": "http://localhost:8080"
      }
    },
    {
      "name": "Launch Application 2",
      "type": "java",
      "request": "launch",
      "mainClass": "com.example.demo.DemoApplication",
      "projectName": "demo",
      "env": {
        "TITLE": "Hello Java!",
        "A1URL": "http://localhost:3002"
      }
    }
  ]
}
